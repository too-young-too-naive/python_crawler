ion(val) {\r\n\r\n                    var newUrl = JSON.parse(decodeURIComponent(val.substr(val.indexOf('{'))));\r\n                    window.location.href = newUrl.url;\r\n                }\r\n            };\r\n        }());\r\n    }\r\n</script>\r\n<span class=\"change_language\" id=\"language-selector-outer\">\r\n    <div class=\"language-selector-wrapper\">\r\n        <i class=\"globe-icon\"></i>\r\n            <select id=\"languageSelector\" name=\"SelectedLanguageName\"><option selected=\"selected\" value=\"zh\">中文 (CHINESE)</option>\r\n<option value=\"en{&quot;url&quot;:&quot;https://www.msdmanuals.com/?lforce=true&quot;}\">ENGLISH (英语)</option>\r\n<option value=\"fr{&quot;url&quot;:&quot;https://www.msdmanuals.com/fr?lforce=true&quot;}\">FRAN&#199;AIS (法国)</option>\r\n<option value=\"de{&quot;url&quot;:&quot;https://www.msdmanuals.com/de?lforce=true&quot;}\">Deutsch (德语)</option>\r\n<option value=\"it{&quot;url&quot;:&quot;https://www.msdmanuals.com/it?lforce=true&quot;}\">Italiano (意大利)</option>\r\n<option value=\"ja{&quot;url&quot;:&quot;https://www.msdmanuals.com/ja?lforce=true&quot;}\">日本語 (日本)</option>\r\n<option value=\"ko{&quot;url&quot;:&quot;https://www.msdmanuals.com/ko?lforce=true&quot;}\">한국어 (朝鲜的)</option>\r\n<option value=\"pt{&quot;url&quot;:&quot;https://www.msdmanuals.com/pt?lforce=true&quot;}\">Portugu&#234;s (葡萄牙语)</option>\r\n<option value=\"ru{&quot;url&quot;:&quot;https://www.msdmanuals.com/ru?lforce=true&quot;}\">ру́сский (俄语)</option>\r\n<option value=\"es{&quot;url&quot;:&quot;https://www.msdmanuals.com/es?lforce=true&quot;}\">ESPA&#209;OL (西班牙)</option>\r\n</select>\r\n    </div>\r\n</span>\r\n<script type=\"text/javascript\">\r\n    $().ready(function() {\r\n\r\n        var languageSwitchSelectedOptionValue = $('#languageSelector option[value^=zh]').first().val();\r\n        $('#languageSelector').val(languageSwitchSelectedOptionValue);\r\n\r\n        var languageLinks = $(\".language-selector-wrapper ul.sbOptions\").find(\"a\");\r\n        for (var i = 0; i < languageLinks.length; i++) {\r\n            var link = $(languageLinks[i]);\r\n            link.removeClass('sbFocus');\r\n            if (link.attr('href').indexOf('#' + languageSwitchSelectedOptionValue) === 0) {\r\n                link.addClass('sbFocus');\r\n                link.unbind('click');\r\n                $('.language-selector-wrapper a.sbSelector').text(link.text());\r\n            }\r\n        }\r\n\r\n    });\r\n</script>\r\n\r\n    </div>\r\n    <h1>哪个版本的 <b>默沙东诊疗手册</b> 更适合你？\r\n    </h1>\r\n    \r\n</header>\r\n<div>\r\n\r\n<script type=\"text/javascript\">\r\n    var shouldDisplayModal = false;\r\n    $(function () {\r\n        if (!shouldDisplayModal)\r\n            return;\r\n        $(\"#preferredLanguageModal\").dialog({\r\n            autoOpen: true,\r\n            dialogClass: 'languageDialog',\r\n            resizable: true,\r\n            width:'auto',\r\n            open: function( event, ui ) {\r\n                $('.main.wrapper').css({\r\n                    '-webkit-filter' : 'blur(15px)',\r\n                    'pointer-events' : 'none'\r\n                });\r\n                $('.main.wrapper').css({'filter' : 'blur(15px)'});\r\n            }\r\n        });\r\n    });\r\n\r\n    function setLanguage(lang) {\r\n        $.ajax({\r\n            url: \"custom/Site/StorePreferredLanguage\",\r\n            type: 'POST',\r\n            dataType: \"json\",\r\n            traditional: true,\r\n            data:  { language: lang },\r\n            success: function (val) {\r\n                $(\"#preferredLanguageModal\").dialog('close');\r\n                $('.main.wrapper').css({'-webkit-filter' : ''});\r\n                $('.main.wrapper').css({'filter' : ''});\r\n                window.location.href = val;\r\n            },\r\n            error: function(jqxhr, textStatus, errorThrown) {\r\n                console.error(errorThrown);\r\n            }\r\n        });\r\n    }\r\n    $(window).resize(function(){\r\n        $(\".languageDialog\").position({\r\n            my: \"center\",\r\n            at: \"center\",\r\n            of: window\r\n        });\r\n    });\r\n\r\n\r\n</script>\r\n\r\n</div>\r\n\r\n<section class=\"hero-images\" role=\"banner\">\r\n    <div class=\"pro hero landing-page-professional-background\">\r\n        <div class=\"hidden_box\">\r\n            <h3>简单即真理</h3>\r\n<div>目前临床信息量每 18 个月翻一番，而且更新步伐只会更快。</div>\r\n<div>在这里占据医学前沿。</div>            \r\n        </div>\r\n        <div class=\"box\">\r\n            \r\n            \r\n<div class=\"logo-wrapper single-line inverted professional\">\r\n    <a class=\"site-logo-control clear-edition-switch\" href=\"#\">\r\n            <span class=\"text-logo first\">默沙东</span><span class=\"text-logo second\">诊疗手册 </span>\r\n\r\n    </a>\r\n            <a class=\"text-edition\">医学专业人士版</a>\r\n</div>\r\n<script>\r\n    $(\".clear-edition-switch\").click(function (event) {\r\n        event.preventDefault();\r\n            window.location = \"/zh/\";\r\n        return false;\r\n    })\r\n</script>\r\n            <div class=\"box-content\">\r\n                <h3>我是医学从业人员或医学学生。</h3>\r\n<p>我需要更精深的内容</p>  \r\n                <a data-id=\"PE\" class=\"editionSwitch btn\" href=\"/zh/专业\" class=\"btn\">进入专业版</a>\r\n                <a data-id=\"PE\" class=\"editionSwitchMobile btn\" onclick=\"MManual.editionSwitch.makeSwitch(this)\" href=\"/zh/专业\" class=\"btn\">进入</a>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"home hero landing-page-home-background\">\r\n        <div class=\"hidden_box\">\r\n            <h3>简单即真理</h3>\r\n<div>世界上最有效的药物是知识，但是错误的信息可能危害您的健康。</div>\r\n<div>避免杂乱无章，就此走上正确的学习之路。</div>\r\n       \r\n        </div>\r\n        <div class=\"box\">\r\n            \r\n<div class=\"logo-wrapper single-line normal consumer\">\r\n    <a class=\"site-logo-control clear-edition-switch\" href=\"#\">\r\n            <span class=\"text-logo first\">默沙东</span><span class=\"text-logo second\">诊疗手册 </span>\r\n\r\n    </a>\r\n            <a class=\"text-edition\">大众版</a>\r\n</div>\r\n<script>\r\n    $(\".clear-edition-switch\").click(function (event) {\r\n        event.preventDefault();\r\n            window.location = \"/zh/\";\r\n        return false;\r\n    })\r\n</script>\r\n            <div class=\"box-content\">\r\n                <h3>我是病人、父母或护理人员。</h3>\r\n<p>我需要通俗易懂的内容。</p>  \r\n                <a data-id=\"HE\" class=\"editionSwitch btn\" href=\"/zh/首页\" class=\"btn\">进入家庭版</a>\r\n                <a data-id=\"HE\" class=\"editionSwitchMobile btn\" onclick=\"MManual.editionSwitch.makeSwitch(this)\" href=\"/zh/首页\" class=\"btn\">进入</a>\r\n            </div>\r\n        </div>\r\n    </div>\r\n                \r\n</section>\r\n\r\n<section class=\"content\">\r\n\r\n    <aside>\r\n            <img src=\"/~/media/manual/landing/msd-gmk-2020-english_msd_zhcn.png\" alt=\"GMK 2020\"/>\r\n    </aside>\r\n                \r\n    <article class=\"body\">\r\n        <div>\r\n            <h2>我们的使命简单明了：</h2>\r\n<p>本手册，在美国和加拿大称为《默克诊疗手册》，在美加以外地区称为《默沙东诊疗手册》，是世界上使用最广泛的医学信息资源之一。 本手册致力于到 2020 年向各大洲高达 30 亿卫生保健专业人员和病人提供当前最佳的医学信息。</p>\r\n<p>我们坚信，取得健康信息是人人都应享受的权利，并且人人都有权获得准确和易得的医学信息。 我们有责任保护、保存并分享当前最佳的医学信息，促成明智的决定，增进病人和专业医护人员之间的关系，改善全世界医疗保健成果。</p>\r\n<p>因此，我们致力于以多种语言，用电子版形式免费向全球专业医护人员和病人提供手册。</p>\r\n        </div>\r\n    </article>\r\n</section>\r\n    \r\n\r\n                        \r\n            <div id=\"footer\">\r\n            \r\n                \r\n\r\n<div class=\"page-footer\">\r\n    <div class=\"wrapper\">\r\n        <div class=\"foot-logo\">\r\n                <img src=\"/Content/Images/msd_foot_logo.png\" alt=MManual class=\"badge msd\" />\r\n        </div>\r\n       \r\n        <div class=\"foot-note\">\r\n            <h2>默沙东&nbsp;和《默沙东诊疗手册》</h2>\r\n            <div class=\"footer-frame\">\r\n<p>美国新泽西州凯尼尔沃思 Merck and Co., Inc.（美加以外地区称为默沙东）是努力改善全球福祉的健康护理引领者。从开发治疗和预防疾病的新疗法，到满足人们需求，我们致力于改善全世界人们的健康和福祉。&nbsp;本手册于 1899 年作为一项社区服务首次出版。&nbsp;此宝贵资源将继续传承下去，在美加拿大地区被称作《默克诊疗手册》，在世界其余地区被称作《默沙东诊疗手册》。详细了解我们对<a href=\"/zh/专业/resourcespages/global-medical-knowledge-2020\">全球医学知识 2020</a> 的承诺。</p>\r\n<p>&nbsp;</p>\r\n<p><em></em></p>\r\n<em></em></div>\r\n<div class=\"tabelet\">\r\n<img alt=\"默克手册移动设备版 \" src=\"/~/media/images/teblet-zestoopki.png?h=261&amp;w=477&la=zh\"/>\r\n<p>&nbsp;</p>\r\n<p>&nbsp;</p>\r\n</div>\r\n        </div>\r\n        <nav class=\"foot-nav msd zh\">\r\n\r\n            \r\n<div class=\"logo-wrapper multi-line normal  no-edition\">\r\n    <a class=\"site-logo-control clear-edition-switch\" href=\"#\">\r\n            <span class=\"text-logo first\">默沙东</span><span class=\"text-logo second\">诊疗手册 </span>\r\n\r\n    </a>\r\n</div>\r\n<script>\r\n    $(\".clear-edition-switch\").click(function (event) {\r\n        event.preventDefault();\r\n            window.location = \"/zh/\";\r\n        return false;\r\n    })\r\n</script>\r\n\r\n            \r\n            <ul>\r\n                    <li><a href=\"/zh/专业/resourcespages/about-the-manuals\" target=\"\" class=\"About\"><span>关于</span></a></li>\r\n                    <li><a href=\"/zh/专业/resourcespages/disclaimer\" target=\"\" class=\"Disclaimer\"><span>免责声明</span></a></li>\r\n                    <li><a href=\"/zh/专业/content/permissions\" target=\"\" class=\"Permissions\"><span>许可权限</span></a></li>\r\n                    <li><a href=\"/zh/专业/resourcespages/privacy\" target=\"\" class=\"Privacy\"><span>隐私</span></a></li>\r\n                    <li><a href=\"/zh/专业/resourcespages/contributors\" target=\"\" class=\"Contributors\"><span>编者</span></a></li>\r\n                    <li><a href=\"/zh/专业/content/termsofuse\" target=\"_blank\" class=\"Terms of use\"><span>使用条款</span></a></li>\r\n                    <li><a href=\"/zh/专业/content/licensing\" target=\"\" class=\"Licensing\"><span>许可授权</span></a></li>\r\n                    <li><a href=\"/zh/专业/content/contact-us\" target=\"\" class=\"Contact Us\"><span>联系我们</span></a></li>\r\n                    <li><a href=\"/zh/